{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "lib",
    "target": "es2022",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictBindCallApply": true,
    "types": [
      "yml-register/types"
    ]
  },
  "include": [
    "src"
  ]
}
